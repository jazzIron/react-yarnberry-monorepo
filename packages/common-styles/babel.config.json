{
    "plugins": [
        "@babel/plugin-transform-runtime"
    ],
    "presets": [
        [
            "@babel/env",
            {
                "shippedProposals": true
            }
        ],
        [
            "@babel/react",
            {
                "runtime": "automatic",
                "importSource": "@emotion/react"
            }
        ],
        [
            "@babel/typescript",
            {
                "onlyRemoveTypeImports": true
            }
        ]
    ]
}